<h1>検索結果</h1>
<p>検索キーワード：<%= @search_keyword %></p>

<%= will_paginate %>

<ul class="shops">
  <% @shops.each do |shop| %>
    <li>
      <section>
        <h4><%= link_to shop.name, shop %></h4>
        <address><%= "#{Prefecture.find_by(id: shop.prefecture_id).name} #{shop.address}" %></address>
      </section>
    </li>
  <% end %>
</ul>

<%= will_paginate %>
